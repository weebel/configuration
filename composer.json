{
  "name": "weebel/configuration",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "weebel/contracts": "dev-master"
  },
  "autoload": {
    "files": [
      "src//helpers.php"
    ],
    "psr-4": {
      "Weebel\\Configuration\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Weebel\\Configuration\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "symfony/var-dumper": "^6.0",
    "phpunit/phpunit": "^9.5",
    "friendsofphp/php-cs-fixer": "^3.13",
    "mockery/mockery": "^1.5"
  }
}
